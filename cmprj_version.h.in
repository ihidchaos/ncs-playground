#ifndef _CMPRJ_VERSION_H_
#define _CMPRJ_VERSION_H_

#cmakedefine CMPRJ_VERSION_CODE @CMPRJ_VERSION_CODE@
#define CMPRJ_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))

#define CMPRJ_VERSION_NUMBER  @CMPRJ_VERSION_NUMBER@
#define CMPRJ_VERSION_MAJOR   @CMPRJ_VERSION_MAJOR@
#define CMPRJ_VERSION_MINOR   @CMPRJ_VERSION_MINOR@
#define CMPRJ_PATCHLEVEL      @CMPRJ_VERSION_PATCH@
#define CMPRJ_VERSION_STRING  "@CMPRJ_VERSION@"

#endif /* _CMPRJ_VERSION_H_ */
